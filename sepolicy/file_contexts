# Persist filesystem
/dev/block/platform/msm_sdcc\.1/by-name/persist         u:object_r:persist_block_device:s0

# FRP partition
/dev/block/bootdevice/by-name/config                    u:object_r:frp_block_device:s0

# Old camera socket
/data/cam_socket1                                       u:object_r:camera_socket:s0
/data/cam_socket2                                       u:object_r:camera_socket:s0

# Old slim_ap executable
/system/vendor/bin/slim_ap_daemon                       u:object_r:location_exec:s0

# BT/WiFi MAC
/persist/.genmac                                        u:object_r:wifi_data_file:s0
/persist/.bt_nv.bin                                     u:object_r:bluetooth_data_file:s0

# Baseband
/system/etc/set_baseband.sh                         u:object_r:set_baseband_exec:s0

# Backlight
/sys/class/leds/lcd-backlight/brightness                u:object_r:touchscreen_file:s0

# Thermal-engine
/system/bin/thermal-engine                              u:object_r:thermal-engine_exec:s0